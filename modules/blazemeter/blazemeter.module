<?php

/**
 * @file
 * Contains blazemeter.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function blazemeter_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the blazemeter module.
    case 'help.page.blazemeter':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Integration with BlazeMeter') . '</p>';
      return $output;

    default:
  }
}


